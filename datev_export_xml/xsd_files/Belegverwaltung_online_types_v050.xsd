                    value="Abschlagsrechnung|Bargutschrift|Barrechnung|Barrechnungskorrektur|Gutschrift § 14 UStG|Gutschrift/Rechnungskorrektur|Rechnung|Rechnungskorrektur|Schlussrechnung"></xsd:pattern>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p5">
        <xsd:annotation>
            <xsd:documentation>Name: Globale Lokationsnummer</xsd:documentation>
            <xsd:documentation>English Name: Global Location Number</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                GLN-Angabe (Global Location Number) Beschreibung lt. standardisierter GLN z.B. auf GS1-Homepage;
            </xsd:documentation>
            <xsd:documentation>
                Description:
                GLN (Global Location Number) description re standardised GLN e.g., on GS1 homepage
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:long">
            <xsd:totalDigits value="13"/>
            <xsd:minInclusive value="0000000000000"/>
            <xsd:maxInclusive value="9999999999998"/>
            <xsd:pattern value="[\+\-]?(\d{1,13})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p6">
        <xsd:annotation>
            <xsd:documentation>Name: Prozentsatz in Relation zum Maximum 2.2</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                DER PROZENTSATZ IN RELATION ZUM MAXIMUM IST DER ANTEIL IN PROZENT, DER VOM MAXIMUM (=100) GEGEBEN IST.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                Input of a percentage. The percentage states the ratio of a given value (=100) to the value under
                consideration. value 0.00 not permitted; no prefixed sign permitted
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="4"/>
            <xsd:fractionDigits value="2"/>
            <xsd:minInclusive value="0.00"/>
            <xsd:maxInclusive value="99.99"/>
            <xsd:pattern value="[\+\-]?([1-9]\d{0,1}|0)(\.\d{2})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p7">
        <xsd:annotation>
            <xsd:documentation>Name: Betrag</xsd:documentation>
            <xsd:documentation>English Name: Amount</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Betragsfelder für alle Betragseingabemöglichkeiten (z.B. Rechnungsbetrag, Steuerbertrag …); Betrag 0.00
                nicht zulässig; Vorzeichen vor Betrag und nur Minus verwenden
            </xsd:documentation>
            <xsd:documentation>
                Description:
                Amount fields for all amount entry options (e.g., invoice amount, tax amount, etc.); amount of 0.00 not
                permitted; sign before amount and only use minus
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:decimal">
                            <xsd:totalDigits value="12"/>
                            <xsd:fractionDigits value="2"/>
                            <xsd:minInclusive value="0.01"/>
                            <xsd:maxInclusive value="9999999999.99"/>
                            <xsd:pattern value="[\+\-]?([1-9]\d{0,9}|0)(\.\d{2})"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:decimal">
                            <xsd:totalDigits value="12"/>
                            <xsd:fractionDigits value="2"/>
                            <xsd:minInclusive value="-9999999999.99"/>
                            <xsd:maxInclusive value="-0.01"/>
                            <xsd:pattern value="[\+\-]?([1-9]\d{0,9}|0)(\.\d{2})"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:restriction>
    </xsd:simpleType>

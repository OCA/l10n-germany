                5.0 = 5.0
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="3"/>
            <xsd:pattern value="5.0"></xsd:pattern>
        </xsd:restriction>
    </xsd:simpleType>

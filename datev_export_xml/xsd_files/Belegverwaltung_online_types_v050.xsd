    <xsd:simpleType name="p10016">
        <xsd:annotation>
            <xsd:documentation>Name: Bezeichnung 060</xsd:documentation>
            <xsd:documentation>English Name: Designation 060</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Allgemeine Bezeichnung, Benennung, Begriff.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                General designation, name, heading
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                -
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="60"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10017">
        <xsd:annotation>
            <xsd:documentation>Name: Postfach</xsd:documentation>
            <xsd:documentation>English Name: PO Box</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Kennung für ein vom Adressaten bei einem Postamt gemietetes Postschliesfach. Es können Auslands- und
                Inlandspostfächer eingetragen werden.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                Code for a post office box rented from a post office by the addressee. Both foreign and domestic PO
                boxes may be entered.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                2345, 123, 643243, 798xy98763, etc.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="10"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10018">
        <xsd:annotation>
            <xsd:documentation>Name: Postleitzahl International</xsd:documentation>
            <xsd:documentation>English Name: International postcode</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Kennzahl eines Ortes für den postalen Brief- und Paketdienst innerhalb eines Landes.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                A place code for the postal letters and parcel service within a country.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                Zentrale DatenelementId für die nationalen Ersetzungen
                Richtet sich in der Länge nach CZ
                DE = 94, AT = 97, CZ = 541, IT = 96, PL = 98
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="10"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10019">
        <xsd:annotation>
            <xsd:documentation>Name: Ortsname</xsd:documentation>
            <xsd:documentation>English Name: Place name</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Stadt bzw. Gemeindenamen.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                City, town or local authority names.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                AACHEN, NUERNBERG, OLCHING, ROSENHEIM, SCHLIERSEE, ZUERICH, ETC.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="30"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10020">
        <xsd:annotation>
            <xsd:documentation>Name: Prozentsatz 2.2</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                DER PROZENTSATZ BESTIMMT DAS VERHÄLTNIS EINER GEGEBENEN GRÖßE (=100) ZU EINER BETRACHTETEN GRÖßE.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The percentage (%) states the ratio of a given value (=100) to the value under consideration.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="4"/>
            <xsd:fractionDigits value="2"/>
            <xsd:minInclusive value="-99.99"/>
            <xsd:maxInclusive value="+99.99"/>
            <xsd:pattern value="[\+\-]?([1-9]\d{0,1}|0)(\.\d{2})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10023">
        <xsd:annotation>
            <xsd:documentation>Name: Steuernummer</xsd:documentation>
            <xsd:documentation>English Name: Tax number</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Die Steuernummer identifiziert einen Steuerpflichtigen bei seinem zuständigen Finanzamt. Der
                Steuerpflichtige hat pro Steuerart eine Steuernummer.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The tax number identifies taxpayers to their respective tax office. Taxpayers have one tax number per
                tax type.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                450045045, 3343J/43223, ETC.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10025">
        <xsd:annotation>
            <xsd:documentation>Name: Straße und Hausnummer International</xsd:documentation>
            <xsd:documentation>English Name: Street with house number International</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Name einer Straße in einer Stadt/Ort, in der sich ein Gebäude befindet, und die Nummer des Gebäudes in
                dieser Straße.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                Name of a street in the city/town/place where the building is located, plus the number of the building
                in that street.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                richtet sich nach Polen:
                Zentrale DEId
                PL: 135, DE: 136, AT: 136, IT: 134

                SDV Projekt DEId
                PL = 2366, DE: 1411, AT = 1411, IT = 2449
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="41"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10027">
        <xsd:annotation>
            <xsd:documentation>Name: Umsatzsteuer-ID</xsd:documentation>
            <xsd:documentation>English Name: Value added tax ID no.</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Die Umsatzsteuer-Id (USt-IdNr.) ist eine eindeutige EU-weite Kennzeichnung eines
                Umsatzsteuerpflichtigen.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The Value added tax identification number (VATIN) is an identifier used in the European Union Value
                Added Tax for taxation purposes.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                DE833475312, DE938485329, DE93857364, DE039583749, ETC.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="15"/>
            <xsd:pattern value="[0-9a-zA-Z\. _]{1,15}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10028">
        <xsd:annotation>
            <xsd:documentation>Name: Kontonummer Sachkonto</xsd:documentation>
            <xsd:documentation>English Name: Impersonal account number</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Die Kontonummer identifiziert eindeutig ein Konto. Die Kontonummer gilt sowohl für den eigentlichen
                Buchungssatz als auch für die Stammdaten. Für Konto-von und Konto-bis einer Bereichsangabe werden somit
                zwei Datenelemente des Typs Kontonummer verwendet. Die Sachkontolänge kann zwischen 4 und 8 Stellen
                variieren.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The account number uniquely identifies an account. The account number applies both to the actual
                accounting record and to the master data. Thus, two data elements of the type ‘account number’ are used
                for the Account from and the Account to of an area statement. The impersonal account length can be from
                4 to 8 characters long.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:totalDigits value="8"/>
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="99999999"/>
            <xsd:pattern value="([1-9]\d*|0)"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10029">
        <xsd:annotation>
            <xsd:documentation>Name: Datum</xsd:documentation>
            <xsd:documentation>English Name: Date</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Das Datum ist eine konkrete Zeitangabe nach dem Kalender mit der Genauigkeit Jahr, Monat und Tag.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The date is a specific calendar-based time statement comprising the year, month and day.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:date">
            <xsd:minInclusive value="1753-01-01"/>
            <xsd:maxInclusive value="9999-12-31"/>
            <xsd:pattern value="[^:Z]*"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10030">
        <xsd:annotation>
            <xsd:documentation>Name: BIC Bank Identifier Code</xsd:documentation>
            <xsd:documentation>English Name: BIC Bank Identifier Code</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Eindeutige Kennung für einen SWIFT-Teilnehmer. BIC (Bank Identifier Code). SWIFT ist die Abkürzung für
                "Society for Worldwide Interbank Financial Telecommunications". Wie aus dem Namen hervorgeht, handelt es
                sich um eine Gesellschaft, die für Telekommunikation zwischen Banken weltweit sorgt.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                Unique code for each SWIFT participant. BIC (Bank Identifier Code). A familiar term in connection with
                foreign payment transactions is SWIFT, which stands for "Society for Worldwide Interbank Financial
                Telecommunications".
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Regeltyp) / Value range (Regular expression)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="11"/>
            <xsd:pattern value="([A-Z]{4}[A-Z]{2}([A-Z0-9]){2}([A-Z0-9]){0,3})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10031">
        <xsd:annotation>
            <xsd:documentation>Name: Rufnummer Teilnehmer Telefon und Fax</xsd:documentation>
            <xsd:documentation>English Name: Telephone and fax number</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Die Rufnummer ist eine Kommunikationsnummer für ein Kommunikationsmedium wie z.B. Telefon und Fax,
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The number is for communications using media such as telephone or fax.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                08123999999, 0911223322, 0041833422222, ETC.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="20"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10032">
        <xsd:annotation>
            <xsd:documentation>Name: E-Mail Adresse</xsd:documentation>
            <xsd:documentation>English Name: Email address</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Eine E-Mail-Adresse ist die eindeutige Absender- und Empfängeradresse im E-Mail-Verkehr. Eine
                E-Mail-Adresse, wie sie für den Transport per SMTP im Internet verwendet wird, besteht aus zwei Teilen:
                Einem lokalen Teil, im Englischen local-part genannt (steht vor dem @-Zeichen) und einem globalen Teil,
                im Englischen domain-part genannt (steht nach dem @-Zeichen).
            </xsd:documentation>
            <xsd:documentation>
                Description:
                An email address is the unique sender and recipient address used for email communications. An email
                address as used for SMTP Internet transmissions consists of two parts: the local part found to the left
                of the ‘@’ character plus a global domain part to the right of the ‘@’ character.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Regeltyp) / Value range (Regular expression)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="60"/>
            <xsd:pattern value="[^@]+@([^@\.\s]+\.)+([^@\.\s]{2,})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10033">
        <xsd:annotation>
            <xsd:documentation>Name: Berichtigungs-/Umsatzsteuerschlüssel</xsd:documentation>
            <xsd:documentation>English Name: Adjustment code/value added tax code</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Durch den Berichtigungs-/Umsatzsteuerschlüssel wird angegeben, ob es sich bei dem komplexen Buchungssatz
                um Berichtigungsbuchungen oder Umsatzsteuerbuchungen handelt. Der Berichtigungs-/Umsatzsteuerschlüssel
                hat folgende Form: Die erste Stelle enthält den Berichtigungsschlüssel. Wenn kein Berichtigungsschlüssel
                benötigt wird, ist die erste Stelle leer (Blank). Die zweite Stelle enthält den Umsatzsteuerschlüssel.
                Wenn kein Umsatzsteuerschlüssel benötigt wird, wird an zweiter Stelle eine 0 eingetragen. Detailliertere
                Hinweise zu Berichtigungs-/Umsatzsteuerschlüsseln finden sie im FIBU-Handbuch.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The adjustment code or VAT code indicates whether a complex accounting record relates to adjustment
                accounting or VAT accounting. The adjustment code/VAT code takes the following form: the first space
                contains the adjustment code. If no adjustment code is needed this space is left blank. The second space
                contains the VAT code. If no VAT code is needed a zero is entered in the second space. Details of the
                adjustment and VAT codes are given in the FIBU manual.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:unsignedShort">
            <xsd:totalDigits value="4"/>
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="9999"/>
            <xsd:pattern value="([1-9]\d*|0)"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10036">
        <xsd:annotation>
            <xsd:documentation>Name: Buchungstext 60stellig</xsd:documentation>
            <xsd:documentation>English Name: Booking text</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                DER BUCHUNGSTEXT IST EIN INDIVIDUELLER TEXT ZU EINEM BUCHUNGSSATZ.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The booking text is the individual text accompanying an accounting record. Al characters permitted.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Exemplartyp) / Value range (Example):
                Aushilfslöhne, Fahrgeldzuschuß, etc.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="60"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10039">
        <xsd:annotation>
            <xsd:documentation>Name: Kontonummer in der Buchhaltung</xsd:documentation>
            <xsd:documentation>English Name: account number for bookkeeping</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Die Kontonummer identifiziert eindeutig ein Konto innerhalb der Buchhaltung. Die Kontonummer gilt sowohl
                für den eigentlichen Buchungssatz als auch für die Stammdaten (S,K,I). Für Konto-von und Konto-bis einer
                Bereichsangabe werden somit zwei Datenelemente des Typs KONTONUMMER verwendet.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                doesn't exist in english
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Bereichstyp) / Value range (Range)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:totalDigits value="9"/>
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="999999999"/>
            <xsd:pattern value="([1-9]\d*|0)"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10040">
        <xsd:annotation>
            <xsd:documentation>Name: Belegnummer der Länge 36</xsd:documentation>
            <xsd:documentation>English Name: Voucher number</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Identifiziert einen Beleg. Ein Beleg ist z.B. eine Rechnung. Entspricht dem Belegfeld1.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                Uniquely identifies a document according to GOB. A document is, for instance, an invoice and corresponds
                to the invoice number. Corresponds to Document Field 1 in the DATEV bookkeeping system.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Regeltyp) / Value range (Regular expression)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="36"/>
            <xsd:pattern value="[a-zA-Z0-9$%&amp;\*\+\-/]{1,36}"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="p10041">
        <xsd:annotation>
            <xsd:documentation>Name: Kostenstellennummer Länge 36</xsd:documentation>
            <xsd:documentation>
                Beschreibung:
                Kostenstelle des Rechnungswesens.
            </xsd:documentation>
            <xsd:documentation>
                Description:
                The cost centre number identifies a cost centre. A cost centre is a delimited sphere of responsibility,
                a sub-unit of the overall business for accounting purposes for which a cost burden is calculated.
            </xsd:documentation>
            <xsd:documentation>
                Wertebereich (Regeltyp) / Value range (Regular expression)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="36"/>
            <xsd:pattern value="([\S][\S ]{0,34}[\S]|[\S]{0,1})"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="noTYPE">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
</xsd:schema>
